@using System.Globalization
@using WebApp1.Models
@model WebApp1.Models.User
@{
    ViewBag.Title = "ManagerReport";
}

<h1></h1>
<h1>Manager Report</h1>
<h1></h1>
<div class="form-group">
<table class='table'>
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Last Login</th>
        <th>Login Count</th>
    </tr>
    </thead>
    <tbody>
        @foreach (User user in WebApp1.Models.User.GetUsers(Model.Email))
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.LastLoginTime.ToString("MMM dd, yyyy", CultureInfo.InvariantCulture)</td>
                <td>@user.LoginCount</td>
            </tr>
        }
    </tbody>
</table>
</div>

